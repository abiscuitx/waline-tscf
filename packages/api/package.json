{
  "name": "@waline-tscf/api",
  "version": "2.1.0-beta.1",
  "description": "Waline-TSCF Api —— Based on Waline, compatible with Tencent Cloud SCF Deployment.",
  "keywords": [
    "api",
    "waline",
    "tscf",
    "typescript",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/abiscuitx/waline-tscf.git",
    "directory": "packages/api"
  },
  "homepage": "https://github.com/abiscuitx/waline-tscf",
  "bugs": {
    "url": "https://github.com/abiscuitx/waline-tscf/issues"
  },
  "license": "MIT",
  "author": "abiscuitx",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js",
      "require": "./dist/api.cjs",
      "default": "./dist/api.js"
    }
  },
  "main": "./dist/api.cjs",
  "module": "./dist/api.js",
  "browser": "./dist/api.umd.js",
  "types": "./dist/api.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf ./dist",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
