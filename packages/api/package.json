{
  "name": "@waline-tscf/api",
  "version": "1.0.0-Beta.3",
  "description": "A comment system api compatible with Tencent SCF deployment based on Waline",
  "keywords": [
    "api",
    "waline",
    "tscf",
    "typescript",
    "client"
  ],
  "homepage": "https://github.com/abiscuitx/waline-tscf",
  "bugs": {
    "url": "https://github.com/abiscuitx/waline-tscf/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abiscuitx/waline-tscf.git",
    "directory": "packages/api"
  },
  "license": "MIT",
  "author": "abiscuitx",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js",
      "require": "./dist/api.cjs",
      "default": "./dist/api.js"
    }
  },
  "main": "./dist/api.cjs",
  "module": "./dist/api.js",
  "browser": "./dist/api.umd.js",
  "types": "./dist/api.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf ./dist",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
